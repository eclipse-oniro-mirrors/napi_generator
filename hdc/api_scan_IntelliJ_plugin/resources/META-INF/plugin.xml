<idea-plugin>
    <id>com.kh.scan</id>
    <name>API Scan </name>
    <version>1.0</version>
    <vendor email="zhaojunxia@kaihong.com" url="http://kaihong.com/">深圳开鸿数字产业发展有限公司</vendor>

    <idea-version since-build="113.0"/>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <group id="ApiScan.meneGen" text="API Scan">
            <add-to-group group-id="ToolsMenu" anchor="last"/>
        </group>

        <action class="com.kh.scan.ng.ApiScanMenu" id="apiScan" text="API Scan">
            <add-to-group group-id="ApiScan.meneGen" anchor="last"/>
            <keyboard-shortcut keymap="$default" first-keystroke="shift alt Y"/>
        </action>

    </actions>

</idea-plugin>